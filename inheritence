class Animal {
    String name;

    void eat() {
        System.out.println(name + " is eating.");
    }
}

class Dog extends Animal {
    void bark() {
        System.out.println(name + " is barking.");
    }
}

public class Main {
    public static void main(String[] args) {
        Dog dog = new Dog();
        dog.name = "Buddy";
        dog.eat();
        dog.bark();
    }
}



class Vehicle {
    String type;

    void move() {
        System.out.println(type + " is moving.");
    }
}

class Car extends Vehicle {
    void honk() {
        System.out.println(type + " is honking.");
    }
}

public class Main {
    public static void main(String[] args) {
        Car car = new Car();
        car.type = "Car";
        car.move();
        car.honk();
    }
}
